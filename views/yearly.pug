extends layout

block scripts
  //Global site tag (gtag.js) - Google Analytics
  script(src="https://www.googletagmanager.com/gtag/js?id=UA-112717734-1" )
  script(src="/javascripts/analytics.js")
  link(rel='stylesheet', href='/stylesheets/yearly.css')
  link(rel="stylesheet", href="/stylesheets/dayColors.css")
  link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js")
  script(src="https://cdnjs.cloudflare.com/ajax/libs/d3/4.4.0/d3.min.js")
  script(src="/javascripts/yearly.js")
block navbar
  if currentUser
    nav.navbar.navbar-default
      .container-fluid
        .navbar-header
          a.navbar-brand(href="#")= title
        .nav.navbar-nav
          a.navbar-text(href="/monthly") Monthly
        .nav.navbar-nav.navbar-right
          p.navbar-text= userName
          a.btn.btn-primary.navbar-btn(href="/logout") Log out
block content
  div(class="pixels")
    div(class="results")
      div(class="js-heatmap")
      div(class="js-months")
    div(class="js-legend")